{
    "name": "sroutier/menu-builder",
    "description": "A laravel package that allows to dynamically build and manage a set of menus and breadcrumb trails.",
    "keywords": [
        "sroutier",
        "menu",
        "laravel",
        "menu-builder"
    ],
    "homepage": "https://github.com/sroutier/menu-builder",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Sebastien Routier",
            "email": "sroutier@gmail.com",
            "homepage": "https://github.com/sroutier",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
      "psr-4": {
            "sroutier\\menu-builder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sroutier\\menu-builder\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
